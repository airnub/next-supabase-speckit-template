{
  "name": "next-supabase-spec-driven-development-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint:spec": "spectral lint srs/app.yaml",
    "docs:gen": "tsx scripts/spec-gen.ts",
    "rtm:build": "tsx scripts/rtm/build-rtm.ts",
    "agent:run": "tsx scripts/agent/run.ts",
