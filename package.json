    "speckit:verify": "pnpm docs:gen && pnpm rtm:build && git diff --exit-code"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@stoplight/spectral-cli": "^6.11.1",
    "@types/node": "^20.11.0",
    "js-yaml": "^4.1.0",
    "playwright": "^1.46.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.0",
    "zod": "^3.23.8"
  }
}
